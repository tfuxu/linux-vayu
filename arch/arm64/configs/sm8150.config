# Qualcomm Snapdragon SM8150 config fragment
CONFIG_LOCALVERSION="-sm8150"
# CONFIG_LOCALVERSION_AUTO is not set

# Peripherals
CONFIG_I2C_QCOM_GENI=y
CONFIG_HID_RMI=m
CONFIG_RMI4_CORE=m
CONFIG_RMI4_I2C=m
CONFIG_RMI4_F55=y

# Samsung touchscreen used on the 7 pro
CONFIG_TOUCHSCREEN_S6SY761=m

CONFIG_SCSI_UFS_QCOM=y
CONFIG_INTERCONNECT_QCOM_SM8150=y
CONFIG_PINCTRL_SM8150=y

# Reboot support better be built-in to work in initramfs
CONFIG_POWER_RESET_QCOM_PON=y

# Clocks
CONFIG_SM_GCC_8150=y
CONFIG_SM_GPUCC_8150=y
CONFIG_SM_VIDEOCC_8150=y

# Phys
CONFIG_PHY_QCOM_QMP=y
CONFIG_PHY_QCOM_QUSB2=y
CONFIG_PHY_QCOM_USB_HS=y
CONFIG_USB_ETH=y
CONFIG_USB_CONFIGFS=y
CONFIG_USB_CONFIGFS_F_HID=y

#pmOS Related
CONFIG_VT=y
CONFIG_CRYPTO_XTS=y
CONFIG_TMPFS_XATTR=y
CONFIG_DM_CRYPT=y
CONFIG_UEVENT_HELPER=y

# helpful for debugging
CONFIG_PSTORE=y
CONFIG_PSTORE_DEFLATE_COMPRESS=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y

# Remoteproc
# CONFIG_RPMSG_CHAR=y
# CONFIG_QCOM_Q6V5_COMMON=y
# CONFIG_QCOM_Q6V5_ADSP=y
# CONFIG_QCOM_Q6V5_MSS=y
