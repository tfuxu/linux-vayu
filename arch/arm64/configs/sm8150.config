# CONFIG_RPMSG_QCOM_GLINK_SMEM=y
# CONFIG_QCOM_SMEM=y
# CONFIG_QCOM_SMEM_STATE=y
# CONFIG_QRTR=y

CONFIG_FB_SIMPLE=y
CONFIG_DRM=y
CONFIG_DRM_MSM=y


